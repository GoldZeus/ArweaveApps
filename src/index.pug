doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0')
    meta(http-equiv='X-UA-Compatible', content='ie=edge')
    title ArweaveApps - Arweave dApps Directory
    meta(name='description', content='ArweaveApps is an Arweave dApps directory built on top of the Arweave blockchain. Share your permaweb!')
    link(rel='icon', type='image/png', href='data:image/png;base64,')
    link(rel='stylesheet', href='styles/main.sass')

  body.grey.lighten-4
    header
      nav
        .nav-wrapper
          .row
            .col.s12.m6.l4
              a(href='#').brand-logo arweave apps
            .col.s12.m6.l4
              | SEARCH | CATEGORY
            .col.s12.m6.l4
              ul.right
                li.hide-on-small-only
                  a(href='#publish').js-go-publish.btn-small.white.black-text.waves-effect Publish/Update dApp

    .container(style='margin-top: 30px')
      #page-home.apage
        .js-app-list.row
          .js-preload-app-list.col.s12.center-align
            .preloader-wrapper.big.active
              .spinner-layer.spinner-blue
                .circle-clipper.left
                  .circle
                .gap-patch
                  .circle
                .circle-clipper.right
                  .circle
              .spinner-layer.spinner-red
                .circle-clipper.left
                  .circle
                .gap-patch
                  .circle
                .circle-clipper.right
                  .circle
              .spinner-layer.spinner-yellow
                .circle-clipper.left
                  .circle
                .gap-patch
                  .circle
                .circle-clipper.right
                  .circle
              .spinner-layer.spinner-green
                .circle-clipper.left
                  .circle
                .gap-patch
                  .circle
                .circle-clipper.right
                  .circle

    script(src='scripts/app.ts')

doctype html
html(lang='en')
    head
        meta(charset='UTF-8')
        meta(name='viewport', content='width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0')
        meta(http-equiv='X-UA-Compatible', content='ie=edge')
        title ArweaveApps - Arweave permaweb index
        meta(name='description', content='ArweaveApps is an Arweave apps directory built on top of the Arweave platform. Share your permaweb now!')
        link(rel='icon', type='image/png', href='data:image/png;base64,')
        link(href='https://fonts.googleapis.com/icon?family=Material+Icons', rel='stylesheet')
        link(rel='stylesheet', href='sass/main.sass')
    body.grey.lighten-4
        header
            nav
                .nav-wrapper.grey.lighten-5
                    a.brand-logo(href='#') ArweaveApps
                    ul.right
                        li.js-page-active(data-page='home')
                            a(href='#') Home
                        //
                          <li class="js-page-active" data-page="categories"><a href="#">Categories</a></li>
                          <li class="js-page-active" data-page="rankings"><a href="#">Rankings</a></li>
                          <li class="js-page-active" data-page="stats"><a href="#">Stats</a></li>
                        li.js-logged-out
                            a.modal-trigger(href='#modal-login') Login
                        li.js-logged-in.hide
                            a(href='#logout') Logout
                        li.hide-on-small-only
                            a.js-go-publish.btn-small.white.black-text.waves-effect(href='#publish') Publish App
        .row.hide-on-med-and-up
            .col.s12.right-align
                a.js-go-publish.btn-small.waves-effect(href='#publish') Publish App
        .container(style='margin-top: 30px')
            // We remove that margin-top when we have the search bar
            #page-home.apage
                .js-app-list.row
                    .js-preload-app-list.col.s12.center-align
                        .preloader-wrapper.big.active
                            .spinner-layer.spinner-blue
                                .circle-clipper.left
                                    .circle
                                .gap-patch
                                    .circle
                                .circle-clipper.right
                                    .circle
                            .spinner-layer.spinner-red
                                .circle-clipper.left
                                    .circle
                                .gap-patch
                                    .circle
                                .circle-clipper.right
                                    .circle
                            .spinner-layer.spinner-yellow
                                .circle-clipper.left
                                    .circle
                                .gap-patch
                                    .circle
                                .circle-clipper.right
                                    .circle
                            .spinner-layer.spinner-green
                                .circle-clipper.left
                                    .circle
                                .gap-patch
                                    .circle
                                .circle-clipper.right
                                    .circle

                        .center-align Loading 0/100 apps
            #page-publish.apage
                h5 Publish your permaweb link
                form#publish-form.row
                    .input-field.col.s12.l8
                        select#link-link
                        label(for='link-link') My Links
                    .input-field.col.s12.l4
                        select#link-category
                        label(for='link-category') Category
                    .input-field.col.s12.l8
                        input#link-title(type='text', data-length='25', required='')
                        label(for='link-title') Link Title (update an app = same title)
                    .col.s12.l4.center-align
                        a#link-icon(href='#')
                            p Click to add icon
                    .input-field.col.s12
                        textarea#link-description.materialize-textarea(data-length='140')
                        label(for='link-description') App Description
                    .col.s12
                        button.btn.waves-effect(type='submit') Publish my permaweb
                input#imgfile.hide(type='file', accept='image/*')
        footer.row
            .col.s12.m6
                | Built with
                img(src='img/like.png', alt='', width='18')
                |  by
                a(href='https://github.com/cedriking', target='_blank', rel='nofollow') Cedriking
            .col.s12.m6.right-align
                a(href='https://arweave.net/27WsYOcNHWweInDArUK94_GsBeuQKPXalsnwVPPRfe4?to=BPr7vrFduuQqqVMu_tftxsScTKUq9ke0rx4q5C9ieQU', target='_blank', rel='nofollow')
                    img(src='img/contact.png', alt='', width='30')
                a(href='https://www.arweave.org/', target='_blank', rel='nofollow')
                    img(src='data:image/png;base64,', width='30')
                a(href='https://github.com/GoldZeus/ArweaveApps', target='_blank', rel='nofollow')
                    img.icon-frame(src='data:image/png;base64,', width='30')
        #modal-login.modal
            .modal-content
                h4 Login
                .row
                    .col.s12
                        .file-input
                            input#keyfile(type='file')
                            #filedescription Drop a keyfile to login

        script(src='scripts/app.ts')